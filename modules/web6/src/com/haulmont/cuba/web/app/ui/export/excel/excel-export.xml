<window
        xmlns="http://schemas.haulmont.com/cuba/5.0/window.xsd"
        class="com.haulmont.cuba.web.app.ui.export.excel.ExcelExportBrowser"
        messagesPack="com.haulmont.cuba.web.app.ui.export.excel"
        caption="msg://caption"
        >

    <dsContext>
    </dsContext>

    <layout expand="table-panel">
        <vbox id="table-panel" expand="exportOptions" spacing="true" margin="true">
            <label id="label1" value="msg://availableColumns" stylename="large"/>
            <optionsGroup id="exportOptions" multiselect="true" captionProperty="caption"/>
            <hbox spacing="true">
                <button id="upBtn" stylename="link"/>
                <button id="downBtn" stylename="link"/>
            </hbox>
            <label/>
            <checkBox id="exportExpandedCheckBox" caption="msg://exportExpanded"/>
        </vbox>

        <hbox id="hbox3" spacing="true">
            <button id="commitBtn"/>
            <button id="closeBtn"/>
        </hbox>
    </layout>
</window>