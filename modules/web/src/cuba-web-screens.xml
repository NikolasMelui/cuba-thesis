<screen-config xmlns="http://www.haulmont.com/schema/cuba/gui/screen-config.xsd">

    <include file="/com/haulmont/cuba/gui/screen-config.xml"/>

    <screen id="core$Server.browse"
            template="/com/haulmont/cuba/web/app/ui/core/server/server-browse.xml"/>

    <screen id="cuba$ExcelExport"
            template="/com/haulmont/cuba/web/app/ui/export/excel/excel-export.xml"/>

    <screen id="settings"
            template="/com/haulmont/cuba/web/app/ui/core/settings/settings-window.xml"/>

    <screen id="feedback"
            template="/com/haulmont/cuba/web/app/ui/core/feedback/feedback-window.xml"/>

    <screen id="logWindow"
            class="com.haulmont.cuba.web.log.LogWindowLauncher"/>

    <screen id="fts$Search"
            class="com.haulmont.fts.web.ui.results.SearchLauncher"/>

    <screen id="fts$SearchResults"
            template="/com/haulmont/fts/web/ui/results/search-results.xml"/>

    <!-- report screens-->
    <screen id="report$Report.browse"
            template="/com/haulmont/cuba/web/ui/report/browse/report-browse.xml"/>

    <screen id="report$Report.edit"
            template="/com/haulmont/cuba/web/ui/report/edit/report-edit.xml"/>

    <screen id="report$Report.fileUploadDialog"
            template="/com/haulmont/cuba/web/ui/report/fileuploaddialog/file-upload-dialog.xml"/>

    <screen id="report$Report.run"
            template="/com/haulmont/cuba/web/ui/report/run/report-run.xml"/>

    <screen id="report$BandDefinition.edit"
            template="/com/haulmont/cuba/web/ui/report/definition/edit/definition-edit.xml"/>

    <screen id="report$inputParameters"
            template="/com/haulmont/cuba/web/ui/report/browse/input-parameters.xml"/>

    <screen id="report$commonLookup"
            template="/com/haulmont/cuba/web/ui/report/browse/common-lookup.xml"/>

    <screen id="report$ReportInputParameter.edit"
            template="/com/haulmont/cuba/web/ui/report/parameter/edit/parameter-edit.xml"/>

    <screen id="report$ReportValueFormat.edit"
            template="/com/haulmont/cuba/web/ui/report/valueformat/edit/format-edit.xml"/>

    <screen id="report$ReportScreen.edit"
            template="/com/haulmont/cuba/web/ui/report/screen/edit/report-screen-edit.xml"/>

    <screen id="report$ReportTemplate.edit"
            template="/com/haulmont/cuba/web/ui/report/template/edit/template-edit.xml"/>

    <screen id="core$Entity.restore"
            template="/com/haulmont/cuba/web/app/ui/core/restore/entity-restore.xml"/>

    <!-- JMX control -->
    <screen id="jmxcontrol$DisplayMbeans"
            template="/com/haulmont/cuba/web/ui/jmxcontrol/browse/display-mbeans.xml"/>

    <screen id="jmxcontrol$InspectMbean"
            template="/com/haulmont/cuba/web/ui/jmxcontrol/inspect/mbean-inspect.xml"/>

    <screen id="jmxcontrol$EditAttribute"
            template="/com/haulmont/cuba/web/ui/jmxcontrol/inspect/attribute/attribute-edit.xml"/>

    <screen id="jmxcontrol$OperationResult"
            template="/com/haulmont/cuba/web/ui/jmxcontrol/inspect/operation/operation-result.xml"/>

    <screen id="sec$ScreenHistory.browse"
            template="/com/haulmont/cuba/web/app/ui/security/history/screen-history-browse.xml"/>

    <screen id="sys$SendingMessage.browse"
            template="/com/haulmont/cuba/web/ui/sendingmessage/browse/sending-message-browse.xml"/>

    <screen id="sys$Categories.browse"
            template="/com/haulmont/cuba/gui/categories/category-browse.xml"/>
</screen-config>
