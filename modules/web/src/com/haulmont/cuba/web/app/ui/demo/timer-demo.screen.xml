<window
        xmlns="http://www.haulmont.com/schema/cuba/gui/window.xsd"
        class="com.haulmont.cuba.web.app.ui.demo.TimerController"
        >
    <metadataContext>
    </metadataContext>

    <dsContext>
    </dsContext>

    <timers>
        <timer delay="10000" repeating="true" onTimer="invoke:onTimer"/>
        <timer id="t1" delay="5000" repeating="true" global="true"/>
